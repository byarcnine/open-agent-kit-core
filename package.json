  "license": "UNLICENSED",
  "private": false,
  "main": "./lib_dist/oak-provider.cjs",
  "module": "./lib_dist/oak-provider.cjs",
  "types": "./lib_dist/lib.d.ts",
  "scripts": {
    "build": "react-router build && npm run build:embed && npm run build:chat && npm run build:chatWidget && npm run build:lib",
    "dev": "react-router dev",
    "build:embed": "vite build --config vite.embed.config.ts",
    "prebuild:chat": "mkdir -p chat_dist && cp chat_module/package.json chat_dist/package.json",
    "build:chat": "vite build --config vite.chat.config.ts",
    "build:chatWidget": "vite build --config vite.chatWidget.config.ts",
    "build:lib": "vite build --config vite.lib.config.ts",
    "build:routes": "vite build --config vite.routes.config.ts",
    "publish:chat": "cd chat_module && npm version patch --no-git-tag-version && cd .. && npm run build:chat && cd chat_dist && npm publish --access public",
    "publish:prepatch": "npm version prepatch && npm run build && npm publish --access public",
    "publish:patch": "npm version patch && npm run build && npm publish --access public",
    "publish:minor": "npm version minor && npm run build && npm publish --access public",
    "publish:major": "npm version major && npm run build && npm publish --access public",
    "publish:alpha": "npm version prerelease --preid=alpha && npm run build && npm publish --tag alpha --access public && git push && git push --tags",
    "generate": "npx prisma generate",
    "migrate:deploy": "npx prisma migrate deploy",
    "prebuild": "npm run generate && node utils/installPlugins.js",
    "lint": "eslint app",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "tsc"
  },
